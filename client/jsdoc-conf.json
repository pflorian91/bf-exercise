{
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "access": "all"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": [
      "source/scripts/"
    ],
    "exclude": [
      "test/*",
      "node_modules/",
      "logs/",
      "coverage/"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
